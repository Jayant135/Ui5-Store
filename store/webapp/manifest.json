{
  "_version": "1.76.0",
  "sap.app": {
    "id": "store",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "2e34e455-9840-47ec-b82a-29ed9a72bfb4"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.143.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "store.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "navContainer",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "store.view",
        "async": true
      },
      "routes": [
        {
          "name": "RouteHome",
          "pattern": ":?query:",
          "target": [
            "TargetHome"
          ]
        },
        {
          "name":"Inventory",
          "pattern":"/inventory",
          "target": ["Inventory"]
        },
        {
          "name":"Po",
          "pattern":"Po",
          "target": ["Po"]
        },
        {
          "name":"So",
          "pattern":"So",
          "target": ["So"]
        },
        {
          "name":"Dashboard",
          "pattern":"Dashboard",
          "target": ["Dashboard"]
        }
      ],
      "targets": {
        "TargetHome": {
          "id": "Home",
          "name": "Home"
        },
        "Inventory":{
          "name": "Inventory"
        },
        "Po":{
          "name": "Po"
        },
        "So":{
          "name": "So"
        },
        "Dashboard":{
          "name": "Dashboard"
        }
      }
    },
    "rootView": {
      "viewName": "store.view.Home",
      "type": "XML",
      "id": "App1",
      "async": true
    }
  }
}
